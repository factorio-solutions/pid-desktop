{
  "extends": "airbnb",
  "parser": "babel-eslint",

  "root": true,
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "ecmaFeatures": {
    "modules": false
  },
  "rules": {
    "react/prefer-stateless-function": [0],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/forbid-prop-types": [0],
    "import/extensions": [0],
    "import/no-extraneous-dependencies": [2, {"devDependencies": true, "optionalDependencies": true}],
    "jsx-a11y/label-has-for": 0,
    "arrow-parens": [2, "as-needed"],
    "no-underscore-dangle": ["error", { "allowAfterThis": true, "allow": ["_id", "__id"] }],
    "no-console": 0,
    "comma-dangle": [2, "never"],
    "max-len": [2, 200, 4],
    "no-else-return": 0,
    "new-cap": 0,
    "prefer-template": 0,
    "arrow-body-style": 0,
    "no-nested-ternary": 0,
    "default-case": 0,
    "semi":  [2, "never"],
    "linebreak-style": 0,
    "react/require-default-props": 0,
    "key-spacing": ["error", { "align": "value" }],
    "no-unused-expressions": ["error", { "allowShortCircuit": true }]
  },
  "not-used": {
    "comma-style": ["error", "first"],
    "array-bracket-spacing": ["error", "always"],
    "no-multi-spaces": ["error", { "exceptions": { "VariableDeclarator": true, "ImportDeclaration": true } }]
  }
}
